<?xml version="1.0" encoding="utf-8"?>
<shortcuts xmlns:android="http://schemas.android.com/apk/res/android">
    <capability android:name="actions.intent.GET_EMERGENCY_SUPPORT">
        <intent
            android:action="android.intent.action.VIEW"
            android:targetPackage="com.life.lifelink"
            android:targetClass="com.life.lifelink.AssistantActivity">
        </intent>
    </capability>

    <capability android:name="actions.intent.OPEN_APP">
        <intent
            android:action="android.intent.action.VIEW"
            android:targetPackage="com.life.lifelink"
            android:targetClass="com.life.lifelink.MainActivity">
        </intent>
    </capability>
</shortcuts>